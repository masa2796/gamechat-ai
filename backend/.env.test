# Test environment configuration for CI/CD
TESTING=true
ENVIRONMENT=test
LOG_LEVEL=CRITICAL
LOG_DIR=./logs

# Mock API keys for testing (not real keys)
OPENAI_API_KEY=test_key_placeholder
UPSTASH_VECTOR_REST_URL=https://test-upstash-url.com
UPSTASH_VECTOR_REST_TOKEN=test_token_placeholder

# Test database configuration
DATABASE_URL=sqlite:///test.db

# CORS configuration for testing
CORS_ORIGINS=["http://localhost:3000","http://127.0.0.1:3000"]

# Disable external API calls in testing
DISABLE_EXTERNAL_APIS=true
